<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="algorithm\encryption\" />
    <Folder Include="algorithm\decryption\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="algorithms.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>algorithms.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="algorithms.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>algorithms.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
